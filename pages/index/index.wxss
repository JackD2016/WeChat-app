/**index.wxss**/
.container{
  background-color: #f3f3f3;
  padding:0;
}
.page{
  width: 100%;
  height: 322rpx;
  box-sizing: border-box;
}
.slide-image{
 width:105%;
 height:322rpx;
}
.form{
	z-index: 10;
position: absolute;
width: 100%;
height: 49rpx;
box-sizing: border-box;
padding: 5px 20px;
}
.form1{
	position: absolute;
	width: 85%;
	height: 39rpx;
	box-sizing: border-box;
	top: 0;
	left: 20rpx;
	border-radius: 20px;
	background-color:#fff;
	margin-top: 2px;
	line-height: 29px;

}
.input1{
	width: 100%;
	height: 29rpx;
	padding-left: 10px;
	margin-top: -3px;
	line-height: 29rpx;
	font-size: 12px;
	box-sizing: border-box;
}
/* .icon_ss{
	width: 10px;
	height: 10px;
	display: inline-block;
	background: red;
} */
.form .login{
	position: absolute;
	top:10rpx;
	right: 20rpx;
	color:#fff;
	font-size: 12px;
}
.serverlist{
	width:100%;
	box-sizing:border-box;
	margin:0 auto;
	border-bottom:1px solid #dadada;
	background-color:#f3f3f3;
}
.serverlist .wraper{
  width: 100%;
  height: 160px;
  margin: 0 auto;
  padding-left: 8px;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content:space-around;
  align-content:space-around;
}
.list{
  width: 20%;
  display: flex;
  font-size: 12px;
  color: #333;
  flex-direction: column;
  align-items:center;
}
.list image{
  width: 50px;
  height: 50px;
  display: block;
  margin-bottom: 8px;
}
.list span{
  text-align: center;
}
.wrap{
	width: 100%;
	font-size: 12px;
	color: #000;
	font-weight: 300;
	background-color: #fff;	
	font-family: "微软雅黑";
}
.toppic{
	width: 100%;
	height: 56px;
	position: relative;

}
.toppic .toppic1{
	width: 100%;
	height: 56px;
}
.toppic .toppic2{
	width: 100px;
	height: 42px;
	position: absolute;
	top:10px;
	left: 135px;
}
.left{
	width: 100%;
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
}
.left_top{
	width: 50%;
	position: relative;
	box-sizing: border-box;
	border-right:1px solid #eee;	
} 
.toppic .text_title{
  text-align: center;
  
  font-size: 30px;
}
.left_top .left_top_left{
	margin-right:30px;
	position: relative;
}
.left_top .descr1{
   position: absolute;
   top:15px;
   left: 10px;
}
.left_top .descr2{
   position: absolute;
   top:35px;
   left: 10px;
}
.left_top>image{
	width: 120px;
	height:150px; 
	float: right;
}
.left1{
	width: 100%;
	display: flex;
}
.left1 .left_buttom{
	position: relative; 
	width: 25%;
	box-sizing: border-box;
	border-right:1px solid #eee;	
}
.left1 .left_buttom .left_top_top{
	position: relative;
	margin-bottom: 50px; 
}
.left1 .left_buttom .left_top_top .descr1{
   position: absolute;
   top:10px;
   left: 10px;
}
.left1 .left_buttom .descr2{
   position: absolute;
   top:30px;
   left: 10px;
     
} 
.left1 .left_buttom>image{
	width: 100%;
	height: 80px;
}